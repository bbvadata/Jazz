	REQUIRE(VOL.alloc_bytes == VOL.max_transactions*sizeof(VolatileTransaction));

	VOL.destroy_transaction(p_txn[0]);
	REQUIRE(p_txn[0] == nullptr);

	REQUIRE(VOL.alloc_bytes == VOL.max_transactions*sizeof(VolatileTransaction));
	REQUIRE(VOL.p_free != nullptr);

	for (int t = 1; t < 17; t++) {

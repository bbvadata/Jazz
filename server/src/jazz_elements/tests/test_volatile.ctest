		for (int i = 0; i < 1000; i++) {
			if (!VOL.aat_is_in_tree(VOL.aat_highest_priority(p_root), p_root))
				REQUIRE(false);
			p_root = VOL.aat_remove(VOL.aat_highest_priority(p_root), p_root);
		}

		REQUIRE(recursive_audit_aa_tree(p_root) == 3000);

		for (int i = 0; i < 1000; i++) {
			if (!VOL.aat_is_in_tree(VOL.aat_lowest_priority(p_root), p_root))
				REQUIRE(false);
			p_root = VOL.aat_remove(VOL.aat_lowest_priority(p_root), p_root);
		}

		REQUIRE(recursive_audit_aa_tree(p_root) == 2000);

		for (int i = 0; i < 1000; i++) {
			if (!VOL.aat_is_in_tree(p_root, p_root))
				REQUIRE(false);
			p_root = VOL.aat_remove(p_root, p_root);
		}

		REQUIRE(recursive_audit_aa_tree(p_root) == 1000);

		for (int i = 0; i < 990; i++) {
			if (!VOL.aat_is_in_tree(VOL.aat_lowest_priority(p_root), p_root))
				REQUIRE(false);
			p_root = VOL.aat_remove(VOL.aat_lowest_priority(p_root), p_root);
		}

		REQUIRE(recursive_audit_aa_tree(p_root) == 10);

		for (int i = 9; i >= 0; i--) {
			REQUIRE(VOL.aat_is_in_tree(p_root, p_root));
			p_root = VOL.aat_remove(p_root, p_root);

			REQUIRE(recursive_audit_aa_tree(p_root) == i);
		}
		REQUIRE(p_root == nullptr);
	}
}



		p_item->priority = 1;

		p_root = VOL.aat_insert(p_item, p_root);

		p_item = &buff[alloc_items++];

		p_item->key_hash = 3;
		p_item->priority = 3;

		p_root = VOL.aat_insert(p_item, p_root);

		REQUIRE(p_root != nullptr);
		REQUIRE(p_root->key_hash == 1);
		REQUIRE(p_root->level == 2);
		REQUIRE(p_root->p_next->key_hash == 3);
		REQUIRE(p_root->p_prev->key_hash == 2);

		p_item = VOL.aat_highest_priority(p_root);

		REQUIRE(p_item != nullptr);
		REQUIRE(p_item->priority == 3);
		REQUIRE(p_item->level	 == 1);

		p_item = VOL.aat_lowest_priority(p_root);

		REQUIRE(p_item != nullptr);
		REQUIRE(p_item->priority == 1);
		REQUIRE(p_item->level	 == 1);

		p_item = &buff[alloc_items++];

		p_item->key_hash = 4;
		p_item->priority = 4;

		p_root = VOL.aat_insert(p_item, p_root);

		p_item = &buff[alloc_items++];

		p_item->key_hash = 5;
		p_item->priority = 5;

		p_root = VOL.aat_insert(p_item, p_root);

		REQUIRE(p_item->times_used == 0x5a5a5a5a);

		REQUIRE(recursive_audit_aa_tree(p_root) == 5);

		REQUIRE(VOL.aat_is_in_tree(p_root, p_root));
		p_root = VOL.aat_remove(p_root, p_root);

		REQUIRE(recursive_audit_aa_tree(p_root) == 4);

		REQUIRE(VOL.aat_is_in_tree(VOL.aat_lowest_priority(p_root), p_root));
		p_root = VOL.aat_remove(VOL.aat_lowest_priority(p_root), p_root);

		REQUIRE(recursive_audit_aa_tree(p_root) == 3);

		REQUIRE(VOL.aat_is_in_tree(VOL.aat_highest_priority(p_root), p_root));

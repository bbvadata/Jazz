			REQUIRE(p_txn[i] != nullptr);
		}
		if (t == 16)
			REQUIRE(VOL.p_free == nullptr);

		for (int i = t - 1; i >= 0; i--) {
			VOL.destroy_transaction(p_txn[i]);
			REQUIRE(p_txn[i] == nullptr);
		}
		REQUIRE(VOL.p_alloc == nullptr);
		REQUIRE(VOL.p_free	!= nullptr);

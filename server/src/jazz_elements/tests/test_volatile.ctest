			int j = lj/RAND_MAX;

			p_item = &buff[j];

			if (!VOL.aat_is_in_tree(p_item, p_root))
				REQUIRE(false);
			p_root = VOL.aat_remove(p_item, p_root);

			p_item->key_hash = 5000 + i;
			p_item->priority = p_item->priority*((i % 9) - 4);

			p_root = VOL.aat_insert(p_item, p_root);
		}

		REQUIRE(recursive_audit_aa_tree(p_root) == 5000);

		for (int i = 0; i < 5000; i++) {

	TensorDim dim_t1 {{500, 10, 0}}, dim_t2 {{500, 1000, 0}};

	REQUIRE(VOL.new_block(p_txn[0], CELL_TYPE_BYTE, dim_t1.dim, FILL_NEW_WITH_ZERO) == SERVICE_NO_ERROR);
